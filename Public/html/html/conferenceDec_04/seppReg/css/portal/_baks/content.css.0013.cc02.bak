html, body {	padding: 0;	font-family: Arial;	line-height: 1.2;	font-size: 12px;}#content, #contentwide {	position: absolute;	top: 170px; left: 50%;	width: 375px;	padding-left: 5px;	padding-right: 5px;	color: #000;	margin-left: -199px;				border-right: 1px dotted #949494;}#contentwide {	width: 565px;	margin-left: -199px;}#headline div{	height: 24px;	background-image: url('textimages/headline.png');	background-repeat: no-repeat;}#headline h2{	display:inline;		color: #000;	padding-left: 5px;	font-size: 18px;	text-transform: lowercase;}#news div.newsheader {        background-image: url('textimages/news.png');        background-repeat: no-repeat;	border-bottom: 7px solid #ffcc33;	padding-bottom: 2px;	margin-bottom: 10px;	margin-top: 0px;}#news div.newsheader div {	border: none;	height: 19px;	padding: 0; margin: 0;}#news h2 {	display: none;}#news p.date{	color: #aaaaaa;	margin: 0; padding:0;	margin-top: 3px;}.blueHeader{color: #003366;}#news a{	color: #336699;}#news p{	padding:0; margin:0;	padding-bottom: 10px;}#register {	display:block;	height: 74px; width:111px;	background-image: url('textimages/bigsignup.png');	background-repeat: no-repeat;}#caracteristics div {	background-image: url('textimages/features.png');	background-repeat: no-repeat;	height: 34px;	margin-top: 30px;	margin-bottom: 20px;}#caracteristics ul {	width: 180px;}#caracteristics li {        color: 666666;         font-size: 14px;	list-style-image: url(images/smalldisc.png);}#caracteristics h2 {	display: none;}#breadcrumbs {	margin-bottom: 5px;}#breadcrumbs a{	text-decoration: none;	color: #336699;}