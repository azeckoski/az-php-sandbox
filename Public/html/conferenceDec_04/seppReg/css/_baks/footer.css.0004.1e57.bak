#footer {	display: block;	font-size: 12px;	text-align: center;	width: 362px;	height: 32px;	background-image: url('../images/footer.png');}#content #footer{	display: inline;}#footer ul{	margin: 0;	padding: 0;	padding-top: 8px;}#footer li{	display: inline;	list-style: none;	padding-left: 4px;	padding-right: 4px;	color: #949494;}#footer li a{	padding-right: 8px;	color: #336699;	text-decoration: none;}