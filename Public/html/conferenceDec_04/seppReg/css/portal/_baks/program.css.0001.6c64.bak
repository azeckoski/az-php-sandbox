/* CSS Document */#program {margin:0px;padding:0px;}table{margin:0px;padding: 0px;font-size:11px;color: #000000;font-family: Arial, Helvetica, sans-serif;}td {padding: 3px 0px;}.date {font-size: 12px;}.plenary{text-align: center;background-color:#FFFF99;border: 1px solid #ccc;}.special{border: 1px solid #ccc;text-align: center;background-color: #CC00CC;}.break {border: 1px solid #ccc;text-align: center;background-color: #CC99FF;}.preConf{border: 1px solid #ccc;text-align: center;background-color:#66CC99;}.open{border: 1px solid #ccc;text-align: center;background-color:#6699CC;}.openTIME{border: 1px solid #ccc;text-align: center;}.spacerRow{border: 0px solid #ccc;}.time {font-size: 10px;white-space:nowrap;font-weight:normal;text-align: right;border:1px solid #ccc;}.sessionNo {font-size:95%;color: #fff;white-space: nowrap;padding: 0px;margin: 0px;}.sessionName{font-weight: bold;}.speaker{font-family:Arial, Helvetica, sans-serif;font-size: 85%;color: #000;}