/* CSS Document */.events {margin: 5px 10px;}.next_item {font-size: 10px;font-style: bold;color:red;align: right;text-align: center;text-transform: lowercase;border: 1px solid #eee;}.prev{text-align: left;padding: 0px 20px;background-image: url(http://www.sakaiproject.org/graphics/prev_arrow.gif);background-position: left;background-repeat: no-repeat;}.next{text-align: right;padding: 0px 20px;background-image: url(http://www.sakaiproject.org/graphics/next_arrow.gif);background-position: right;background-repeat: no-repeat;}#footer {top: 20px;width: 100%;background-color: #ffcc33;	display: block;	font-size: 12px;	text-align: center;	width: 362px;	height: 42px;	}#content #footer{	display: block;}#footer ul{	margin: 0;	padding: 0;	padding-top: 8px;}#footer li{	display: inline;	list-style: none;	padding-left: 4px;	padding-right: 4px;	color: #949494;}#footer li a{	padding-right: 8px;	color: #336699;	text-decoration: none;}