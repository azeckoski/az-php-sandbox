	<params>
		<param name="theme" type="list" default="default" label="Functionality" description="Select functionality">
			<option value="default">Basic</option>
			<option value="advanced">Advanced</option>
		</param>
 		<param name="@spacer" type="spacer" default="" label="" description="" />
 		<param name="text_direction" type="list" default="ltr" label="Text Direction" description="Ability to change text direction">
 			<option value="ltr">Left to Right</option>
 			<option value="rtl">Right to Left</option>
 		</param>
 		<param name="invalid_elements" type="text" default="script,object,applet,iframe" label="Prohibited Elements" description="Elements that will be cleaned from the text"/>
 		<param name="content_css" type="list" default="1" label="Template CSS classes" description="Load CSS classes from template_css.css">
 			<option value="0">No</option>
 			<option value="1">Yes</option>
 		</param>
 		<param name="content_css_custom" type="text" default="" label="Custom CSS Classes" description="You can specify the loading of a custom CSS file - simply enter the FULL path to the css file you want loaded"/>
  		<param name="newlines" type="list" default="false" label="Newlines" description="Newlines will be made into the selected option">
 			<option value="true">BR Elements</option>
 			<option value="false">P Elements</option>
 		</param>
 		<param name="@spacer" type="spacer" default="" label="" description="" />
 		<param name="toolbar" type="list" default="top" label="Toolbar" description="Position of the toolbar">
 			<option value="top">Top</option>
 			<option value="bottom">Bottom</option>
 		</param>
 		<param name="html_height" type="text" default="550" label="Popup Height" description="Height of HTML mode pop-up window - only in Advanced Mode"/>
 		<param name="html_width" type="text" default="750" label="Popup Width" description="Width of HTML mode pop-up window - only in Advanced Mode"/>
 	</params>
</mosinstall>
